{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Aprendendo_csharp/bin/Debug/netcoreapp3.0/Aprendendo_csharp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Aprendendo_csharp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Tournment",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/CS50/Python/Labs/world-cup/tournament.py",
            "justMyCode": true,
            "args": ["${workspaceFolder}/CS50/Python/Labs/world-cup/2018m.csv"]
        }
    ]
}